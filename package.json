{
  "name": "sdk-finance",
  "version": "0.2.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20.10.0"
  },
  "scripts": {
    "dev": "npm run dev -w @sdk5/core",
    "build": "npm run build -w @sdk5/core --",
    "lint": "npm run -ws --if-present --parallel lint",
    "lint:fix": "npm run -ws --if-present lint:fix",
    "prettier:check": "prettier --cache --check",
    "prettier:check-scripts": "prettier --cache --check \"**/*.{js,ts,vue,json,mdx,cjs,mjs}\"",
    "prettier:write": "prettier --cache --write",
    "prettier:write-scripts": "prettier --cache --write \"**/*.{js,ts,vue,json,cjs,mjs}\"",
    "pre-commit": "lint-staged",
    "pre-push": "concurrently --kill-others-on-fail -n LINT,AUDIT \"npm run lint\" \"npm audit\"",
    "storybook:ui-kit": "npm run -w @sdk5/ui-kit storybook",
    "storybook:ui-kit-old": "npm run -w @sdk5/ui-kit-old storybook",
    "postinstall": "rimraf ./node_modules/@types/react; patch-package"
  },
  "dependencies": {
    "@tanstack/vue-query": "^5.28.7",
    "@vueuse/core": "10.9.0",
    "@vueuse/integrations": "^10.7.1",
    "axios": "^1.6.1",
    "classnames": "^2.5.1",
    "dayjs": "^1.11.5",
    "element-ui": "^2.15.10",
    "file-saver": "^2.0.5",
    "lodash": "^4.17.21",
    "sortablejs": "^1.15.1",
    "vue": "^2.7.15",
    "vue-class-component": "^7.2.6",
    "vue-i18n": "^8.27.2",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^3.6.5",
    "vuex": "^3.6.2",
    "vuex-module-decorators": "^1.2.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.6.1",
    "@sdk5/eslint-config": "workspace:*",
    "@sdk5/tsconfig": "workspace:*",
    "@types/file-saver": "^2.0.7",
    "@types/google.maps": "^3.50.4",
    "@types/lodash": "^4.14.185",
    "@types/sortablejs": "^1.15.7",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "patch-package": "^8.0.0",
    "postcss-html": "^1.5.0",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "sass": "^1.63.6",
    "stylelint": "^15.10.1",
    "tailwindcss": "^3.1.8",
    "typescript": "^5.2.2"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run pre-push",
      "pre-commit": "npm run pre-commit",
      "post-commit": "git update-index --again"
    }
  },
  "lint-staged": {
    "*.(js|ts|vue|scss|css|json)": [
      "npm run prettier:write"
    ]
  },
  "workspaces": [
    "apps/*",
    "packages/*",
    "core-modules/*"
  ],
  "overrides": {
    "trim": ">=1.0.1",
    "glob-parent": ">=5.1.2",
    "trim-newlines": "^3.0.0",
    "meow": "^9.0.0",
    "semver": ">=7.5.2",
    "optionator": ">=0.9.3",
    "postcss": "^8.4.31"
  }
}
