{
  "name": "sdk-finance",
  "version": "0.2.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20.10.0"
  },
  "scripts": {
    "dev": "npm run dev -w @sdk5/core",
    "build": "npm run build -w @sdk5/core --",
    "lint": "concurrently --kill-others-on-fail -n lint:script,lint:style \"npm run lint:script\" \"npm run lint:style\"",
    "lint:fix": "concurrently --kill-others-on-fail -n lint:script:fix,lint:style:fix \"npm run lint:script:fix\" \"npm run lint:style:fix\"",
    "lint:script": "eslint --cache -c eslint.config.js .",
    "lint:script:fix": "eslint --cache --fix -c eslint.config.js .",
    "lint:style": "stylelint \"**/*.{vue,scss,sass,css}\"",
    "lint:style:fix": "stylelint --fix \"**/*.{vue,scss,sass,css}\"",
    "prettier:check": "prettier --cache --check",
    "prettier:check-scripts": "prettier --cache --check \"**/*.{js,ts,vue,json,mdx,cjs,mjs}\"",
    "prettier:write": "prettier --cache --write",
    "prettier:write-scripts": "prettier --cache --write \"**/*.{js,ts,vue,json,cjs,mjs}\"",
    "pre-commit": "lint-staged",
    "pre-push": "concurrently --kill-others-on-fail -n LINT,AUDIT \"npm run lint\" \"npm audit\"",
    "storybook:ui-kit": "npm run -w @sdk5/ui-kit storybook",
    "storybook:ui-kit-old": "npm run -w @sdk5/ui-kit-old storybook",
    "postinstall": "rimraf ./node_modules/@types/react; patch-package",
    "prepare": "node .husky/install.mjs"
  },
  "dependencies": {
    "@tanstack/vue-query": "^5.34.2",
    "@vueuse/core": "10.9.0",
    "@vueuse/integrations": "^10.9.0",
    "axios": "^1.6.8",
    "classnames": "^2.5.1",
    "dayjs": "^1.11.10",
    "element-ui": "^2.15.14",
    "file-saver": "^2.0.5",
    "lodash": "^4.17.21",
    "postcss": "^8.4.38",
    "sortablejs": "^1.15.2",
    "vue": "^2.7.16",
    "vue-class-component": "^7.2.6",
    "vue-i18n": "^8.28.2",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^3.6.5",
    "vuex": "^3.6.2",
    "vuex-module-decorators": "^1.2.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.0.2",
    "@eslint/js": "^9.2.0",
    "@tanstack/eslint-plugin-query": "^5.32.1",
    "@tsconfig/node20": "^20.1.4",
    "@types/eslint": "^8.56.10",
    "@types/eslint__js": "^8.42.3",
    "@types/file-saver": "^2.0.7",
    "@types/google.maps": "^3.50.4",
    "@types/lodash": "^4.17.0",
    "@types/node": "^20.12.8",
    "@types/sortablejs": "^1.15.8",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "cosmiconfig": "^9.0.0",
    "cssnano": "^7.0.1",
    "eslint": "^9.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-json-format": "^2.0.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "eslint-plugin-storybook": "^0.6.15",
    "eslint-plugin-vue": "^9.25.0",
    "globals": "^15.1.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "patch-package": "^8.0.0",
    "postcss-html": "^1.6.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "sass": "^1.76.0",
    "stylelint": "^16.5.0",
    "stylelint-config-recommended-scss": "^14.0.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-prettier": "^5.0.0",
    "stylelint-scss": "^6.3.0",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.2.2",
    "typescript-eslint": "^7.8.0",
    "vite-plugin-mkcert": "^1.17.5",
    "vue-eslint-parser": "^9.4.2"
  },
  "lint-staged": {
    "*.(js|ts|vue|scss|css|json)": [
      "npm run prettier:write"
    ]
  },
  "workspaces": [
    "apps/*",
    "packages/*",
    "core-modules/*"
  ],
  "overrides": {
    "trim": ">=1.0.1"
  },
  "packageManager": "npm@10.2.5+sha256.8002e3e7305d2abd4016e1368af48d49b066c269079eeb10a56e7d6598acfdaa"
}
